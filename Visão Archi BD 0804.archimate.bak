  <folder name="Motivation" id="id-f662b095804b48e2b435b1a43d4fac60" type="motivation">
    <element xsi:type="archimate:Stakeholder" name="Diretor de operações de serviços" id="id-03e1f241119c4abba03a865ef16855b6"/>
    <element xsi:type="archimate:Driver" name="Acabar com informalidades ao atendimento aos clienes, coisa que esta gerando insatisfação" id="id-1e2219a5562a4b42b4f4de39a782974b"/>
    <element xsi:type="archimate:Value" name="Reduzir perdas de contratos por insatisfação com o Suporte. Hoje perde-se em média R$280.000,00 por ano com clientes cancelado contrato por motivo do suporte ruim." id="id-463c746fd1944e5fad6d0d4cd1fc207a"/>
    <element xsi:type="archimate:Value" name="Firmar novos contratos com clientes indicados por clinetes satisfeitos. Hoje são firmados em media R$180.000,00 por ano com novos contratos vindos por indicação." id="id-439e20dcb7b64a50bf9d522e14f13fc4"/>
    <element xsi:type="archimate:Assessment" name="Criar um mecanismo de formalização dos chamados tecnicos de suporte usando um sistema digital" id="id-faa310c11abe45ac81c0f3f7b97ff436"/>
    <element xsi:type="archimate:Assessment" name="Criar um mecanismo para que o cliente manisfeste formamalmente seu nivel de satisfação" id="id-876735df7dbd48a791f7893df28f2e28"/>
    <element xsi:type="archimate:Goal" name="Informatizar a abertura de ocorrencias tecnicas" id="id-cf6bb0ae44504d7694154094ae9a40cd"/>
    <element xsi:type="archimate:Meaning" name="Meaning" id="id-4e6c4ede877c4f7fb0ef1909c8b1ebda"/>
    <element xsi:type="archimate:Goal" name="Informatizar o registro do processo de solução" id="id-5ed35cd231f642b89012fa1177637c71"/>
    <element xsi:type="archimate:Goal" name="Informatizar recurso para gerenciamento do serviço de suporte" id="id-021e5771ebfb4e84bb98f5d093db3318"/>
    <element xsi:type="archimate:Goal" name="Informatizar processo de avaliação pelo cliente" id="id-f2d97ea2c7d44a2082548b85c549241e"/>
    <element xsi:type="archimate:Outcome" name="O registro de chamados em sistema vai ser entregue em 31/12/2023" id="id-a38296267b8c4b1f8430a3b9ab671347"/>
    <element xsi:type="archimate:Outcome" name="As consultas gerenciais sobre o atendimento de chamados devem ser liberadas a aprtir do dia 20/01/2024" id="id-863d5d929d644da9a387141c9b0b0647"/>
    <element xsi:type="archimate:Outcome" name="Entregar o sistema de envio de pesquisa e tratamento de avaliações pelos clientes 3 meses após a entrega do registro de chamados em sistema." id="id-cb656e62cd644973bf65bd7ba049030c"/>
    <element xsi:type="archimate:Principle" name="Todod chamado tem que ser protocolado e esse protocolo fornecido ao cliente" id="id-10464f2da2c34c4ca7004e5d1df5d94f"/>
    <element xsi:type="archimate:Principle" name="Registrar o passos ate a solução enviando atualização de status para o cliente." id="id-34aa65e5ad754f01bb73aea2e526de06"/>
    <element xsi:type="archimate:Principle" name="Permitir acompanhar um chamado especifico e o resultado da avaliação média de atendimento" id="id-888cea1effbc4eee95d975bb02a4065f"/>
    <element xsi:type="archimate:Principle" name="Enviar a pesquisa de satisfação para o cliente, a cada chamado fechado(solucionado); porem, o cliente responda a pesquisa por ação." id="id-32943e74925f4ab4ad60d6f2406ac0db"/>
